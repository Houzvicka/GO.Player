<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:GO.UWP.Player.Converters">

    <!-- Converters -->
    <converters:BoolToVisibilityConverter x:Key="BoolToVisibility" />
    <converters:BoolToVisibilityConverter x:Key="InvertedBoolToVisibility" VisibleWhenFalse="True" />
    <converters:ContentToVisibilityConverter x:Key="ContentToVisibility" />
    <converters:ContentToVisibilityConverter x:Key="InvertedContentToVisibility" VisibleWhenFalse="True" />
</ResourceDictionary>
